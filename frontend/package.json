{
  "name": "frontend",
  "proxy": "https://mumbleapi.herokuapp.com/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^27.0.0",
    "@ckeditor/ckeditor5-react": "^3.0.2",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "date-fns": "^2.19.0",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-detect-click-outside": "^1.0.7",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.1",
    "react-image-crop": "^8.6.6",
    "react-linkify": "^1.0.0-alpha",
    "react-placeholder": "^4.1.0",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-toastify": "^7.0.3",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "web-vitals": "^1.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:local": "REACT_APP_API_ENDPOINT=http://127.0.0.1:8000 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "format": "prettier --write \"src/**/*.(js|jsx|ts|tsx|css|scss)\"",
    "format:check": "prettier -c \"src/**/*.(js|jsx|ts|tsx|css|scss)\"",
    "stylelint": "stylelint \"src/**/*.css\"",
    "stylelint:fix": "stylelint \"src/**/*.css\" --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/prop-types": [
        "off"
      ],
      "react/react-in-jsx-scope": [
        "off"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^6.8.0",
    "eslint-plugin-react": "^7.23.1",
    "prettier": "^2.2.1",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "typescript": "^4.2.3"
  }
}
